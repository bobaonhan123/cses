    "n = int(input())\n",
    "\n",
    "while True:\n",
    "    if n==1:\n",
    "        print(1)\n",
    "        break\n",
    "    print(n,end=' ')\n",
    "    if n%2==0:\n",
    "        n //=2\n",
    "    else:\n",
    "        n=n*3+1\n"
